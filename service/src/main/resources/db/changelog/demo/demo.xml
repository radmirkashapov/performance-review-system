                 splitStatements="true"
                 stripComments="true"/>
        <sqlFile encoding="UTF-8"
                 path="db/changelog/demo/02-demo-data-test-check-lists.sql"
                 relativeToChangelogFile="false"
                 splitStatements="true"
                 stripComments="true"/>
        <sqlFile encoding="UTF-8"
                 path="db/changelog/demo/03-demo-data-checklist-questions.sql"
                 relativeToChangelogFile="false"
                 splitStatements="true"
                 stripComments="true"/>

    </changeSet>
</databaseChangeLog>
